{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "(gdb) g++ Launch",
            "type": "cppdbg",
            "request": "launch",
            "program": "${fileDirname}\\${fileBasenameNoExtension}.exe",
            "args": [],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "miDebuggerPath": "C:/Users/Public/tools/w64devkit/bin/gdb.exe",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ],
            "preLaunchTask": "g++ build active file"
        },
        {
            "type": "cmake",
            "request": "launch",
            "name": "CMake Script Debug",
            "cmakeDebugType": "script",
            "scriptPath": "${workspaceFolder}/CMakeLists.txt"
        }
    ]
}
